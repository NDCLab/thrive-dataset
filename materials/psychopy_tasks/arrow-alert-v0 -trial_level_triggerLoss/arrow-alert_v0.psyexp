    <Routine name="task_condition">
      <CodeComponent name="condition_init_blockCounter">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="#initialize the following variables at the start of the condition&amp;#10;blockCounter = 0&amp;#10;&amp;#10;# To skip routines in case of &quot;observe other&quot;,&amp;#10;# we will need to use endTask.&amp;#10;# see begin routine tab for more.&amp;#10;endTask = 0 " valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="blockCounter = 0;&amp;#10;endTask = 0;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="blockCounter = 0;&amp;#10;if ((conditionText === &quot;Observe other&quot;)) {&amp;#10;    endTask = 1;&amp;#10;} else {&amp;#10;    if ((conditionText === &quot;Observed&quot;)) {&amp;#10;        endTask = 0;&amp;#10;    } else {&amp;#10;        if ((conditionText === &quot;Alone&quot;)) {&amp;#10;            endTask = 0;&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="#reset the following variables at the start of the experiment&amp;#10;blockCounter = 0&amp;#10;&amp;#10;&amp;#10;if conditionText == 'Observe other':&amp;#10;    endTask = 1&amp;#10;elif conditionText == 'Observed':&amp;#10;    endTask = 0&amp;#10;elif conditionText == 'Alone':&amp;#10;    endTask = 0" valType="extendedCode" updates="constant" name="Begin Routine"/>
